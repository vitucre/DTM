<?xml version="1.0" encoding="UTF-8"?>
<collection>
<!--本地主机绑定ip:port-->
<!--什么时候需要填写？启动[auto_monitor_log.py]-->
<local_host>127.0.0.1:9998</local_host>
<!--监控的对端主机信息-->
<!--什么时候需要填写？启动[auto_monitor_process.py,monitor.py]-->
<remote_host>
    <!--监控的对端主机ip:port-->
    <ip_port>127.0.0.1:9999</ip_port>
    <!--监控的pid-->
    <monitor_pid>22,23,3306</monitor_pid>
    <!--探测间隔时间，单位秒,默认3600-->
    <detect_interval_time></detect_interval_time>
</remote_host>
<remote_host>
    <ip_port>192.168.6.112:9999</ip_port>
    <monitor_pid>22,23,3306</monitor_pid>
    <detect_interval_time>1800</detect_interval_time>
</remote_host>
</collection>